this.spn_onspin_hour= function(obj:nexacro.Spin,e:nexacro.SpinEventInfo)
{
	const idStr = obj.id.substr(4);
	//기본값 또는 DB값
	const prevalue = this.div_cont.form.div_up.form.components["mae_" + idStr].value;

	let pm = 0;
	if(e.up == true){ // 증가
		pm = 1;
	}else{ // 감소
		pm = -1;
	}
	
	const value = Number(prevalue) + pm;
	
	if(value > 22 || value < 6){ // 입력값이 222이상 또는 6이하 입력안되게 설정	
		return false;
	}
	
	this.div_cont.form.div_up.form.components["mae_" + idStr].set_value(value);
};

this.spn_onspin_min= function(obj:nexacro.Spin,e:nexacro.SpinEventInfo)
{
	const idStr = obj.id.substr(4);
	//기본값 또는 DB값
	const prevalue = this.div_cont.form.div_up.form.components["mae_" + idStr].value;
	
	let pm = 0;
	if(e.up == true){ // 증가
		pm = 1;
	}else{ // 감소
		pm = -1;
	}
	
	const value = Number(prevalue) + pm;
	
	if(value > 59 || value < 0){ // 입력값이 59이상 또는 0이하 입력안되게 설정	
		return false;
	}
	
	this.div_cont.form.div_up.form.components["mae_" + idStr].set_value(value);
};



