};

/***********************************************************************
 * Data Transaction Functions
 ************************************************************************/
// 조회
this.fn_search = function()
{
	const svcId = "search";
	const params = "/commonController.do";
	const inDatasets = "input1=ds_search input2=ds_search";
	const outDatasets = "ds_head=output1 ds_detail=output2";
	const argument = "method=getList "
				   + "sqlId='DAY00602_P01.DAY00602_P01_DS_HEAD_S DAY00602_P01.DAY00602_P01_DS_DETAIL_S ' ";
				   
	const callbackFunc = "fn_callback";

	Sct.transaction(this, svcId, params, inDatasets, outDatasets, argument, callbackFunc);
};

this.fn_pdiv_contentSearch = function(){

	const svcId = "pdiv_contentSearch";
	const params = "/commonController.do";
	const inDatasets = "";
	const outDatasets = "ds_detail_popup=output1";
	const argument = "method=getList "
				   + "sqlId='DAY00602_P01.DAY00602_P01_DS_DETAIL_POPUP_S' "
				   + "sicode=" + this.ds_head.getColumn(this.ds_head.rowposition, "sicode") + " ";
				   
	const callbackFunc = "fn_callback";

	Sct.transaction(this, svcId, params, inDatasets, outDatasets, argument, callbackFunc);
}

this.fn_pdiv_satisfactionSearch = function(){

	const svcId = "fn_pdiv_satisfactionSearch";
