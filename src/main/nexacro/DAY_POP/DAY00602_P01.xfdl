	this.ds_detail.setColumn(this.ds_detail.rowposition, "참여도", this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "참여도"));
	this.ds_detail.setColumn(this.ds_detail.rowposition, "만족도", this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "만족도"));
	this.ds_detail.setColumn(this.ds_detail.rowposition, "수행도", this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "수행도"));
	
	let points = "";
	if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "참여도") == "1"){
		points += "상"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "참여도") == "2"){
		points += "중"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "참여도") == "3"){
		points += "하"
	}
	
	if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "만족도") == "1"){
		points += "상"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "만족도") == "2"){
		points += "중"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "만족도") == "3"){
		points += "하"
	}
	
	if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "수행도") == "1"){
		points += "상"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "수행도") == "2"){
		points += "중"
	}else if(this.ds_detail_satisfaction.getColumn(this.ds_detail_satisfaction.rowposition, "수행도") == "3"){
		points += "하"
	}
	
	this.ds_detail.setColumn(this.ds_detail.rowposition, "참만수", points); 
	this.pdiv_satisfaction.closePopup();
};

