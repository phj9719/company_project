// 팝업 콜백
this.fn_popCallback = function (popid, strDs) {
	if(Sct.Util.isEmpty(strDs)) return;
	const objDs = this.gfn_createReturnDs(this, "ds_Return", strDs);
	switch(popid)
	{
		// 작성자
		case "emp" :
			if (objDs.getColumn(0, "DEL") != "1")
			{
				const name = nexacro.trim(objDs.getColumn(0, "c_name"));
				const dutyname = objDs.getColumn(0, "담당직종");
				const staffid = objDs.getColumn(0, "c_staffid");
				this.ds_head.setColumn(0, "담당자", name);
				this.ds_head.setColumn(0, "담당코드", staffid);
				this.ds_head.setColumn(0, "외부", "0");
				this.ds_head.setColumn(0, "VIEW_MANAGER", name + "     " + staffid);
				this.div_cont.form.div_up.form.sta_manager_ex.set_visible(false);
			}
			else
			{
				this.ds_head.setColumn(0, "담당자", "");
				this.ds_head.setColumn(0, "담당코드", "");
				this.ds_head.setColumn(0, "외부", "0");
				this.ds_head.setColumn(0, "VIEW_MANAGER", "");
				this.div_cont.form.div_up.form.sta_manager_ex.set_visible(false);
			}
			break;
		case "program_search" :
			this.ds_head.setColumn(0, "주제", objDs.getColumn(0, "주제"+objDs.getColumn(0, "number")));
			this.ds_head.setColumn(0, "진행과정", objDs.getColumn(0, "주과정"+objDs.getColumn(0, "number")));
			break;
		case "ex_emp" :
			const name1 = nexacro.trim(objDs.getColumn(0, "e_name"));
			const dutyname1 = objDs.getColumn(0, "e_dutyname");
			const staffid1 = objDs.getColumn(0, "e_code");
			this.ds_head.setColumn(0, "담당자", name1);
			this.ds_head.setColumn(0, "담당코드", staffid1);
			this.ds_head.setColumn(0, "외부", "1");
			this.ds_head.setColumn(0, "VIEW_MANAGER", name1 + "     " + staffid1);
			this.div_cont.form.div_up.form.sta_manager_ex.set_visible(true);
			break;				
		default :
			this.gfn_createReturnDs(this, "ds_Return", strDs);
	}
};
