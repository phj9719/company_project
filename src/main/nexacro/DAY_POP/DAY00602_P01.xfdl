this.fn_checkattend = function(){
	let attend = 0;
	let no_attend = 0;
	let unused = 0;
	
	for(let i = 0; i<this.ds_detail.rowcount; i++){
		let kind = this.ds_detail.getColumn(i, "참석")
		if(kind == "1"){
			attend += 1;
		}else if(kind == "2"){
			no_attend += 1;
		}else if(kind == "3"){
			unused += 1;
		}
	}
	
	this.ds_head.setColumn(this.ds_head.rowposition, "참여", attend);
	this.ds_head.setColumn(this.ds_head.rowposition, "불참", no_attend);
	this.ds_head.setColumn(this.ds_head.rowposition, "미이용", unused);		
	this.ds_head.setColumn(this.ds_head.rowposition, "참가인원", "참여 " + String(attend).padStart(2, "0") + "명/불참 " + no_attend + "명");

};
