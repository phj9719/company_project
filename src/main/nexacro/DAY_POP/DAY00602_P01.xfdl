				   + "healcode=" + this.ds_detail.getColumn(this.ds_detail.rowposition, "healcode") + " ";
				   
	const callbackFunc = "fn_callback";

	Sct.transaction(this, svcId, params, inDatasets, outDatasets, argument, callbackFunc);
}

// 저장
this.fn_save = function()
{
	if (!this.fn_save_pre()) return;

	const svcId = "save";
 	const inDs = "ds_head,ds_detail";
	this.fn_save_com(svcId, inDs);
};

//콜백
this.fn_callback = function(strSvcID, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		if (strSvcID == "search"){
			const name = this.ds_head.getColumn(0, "담당자");
			const staffid = this.ds_head.getColumn(0, "담당코드");
			this.ds_head.setColumn(0, "VIEW_MANAGER", name + "     " + staffid);
			
			this.fn_checkattend();
			
			if(this.ds_head.getColumn(this.ds_head.rowposition, "외부") == 1){
				this.div_cont.form.div_up.form.sta_manager_ex.set_visible(true);
			}
			
		}else if (strSvcID == "save"){
			this.fn_search();
		}else if (strSvcID == "pdiv_contentSearch"){
			this.pdiv_content.trackPopupByComponent(this.div_cont, 735, 260);
		}else if(strSvcID == "fn_pdiv_satisfactionSearch"){
			this.pdiv_satisfaction.trackPopupByComponent(this.div_cont, 735, 370);
		}	
	}
	else{
		this.alert(strErrorMsg);
	}
};

