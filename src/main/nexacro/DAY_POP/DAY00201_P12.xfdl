// 팝업 콜백
this.fn_popCallback = function (popid, strDs) {
	if(Sct.Util.isEmpty(strDs)) return;
	const objDs = this.gfn_createReturnDs(this, "ds_Return", strDs);

	switch(popid)
	{
		//
		case "employee" :
			if (objDs.getColumn(0, "DEL") != "1")
			{
				const name = nexacro.trim(objDs.getColumn(0, "c_name"));
				const staffid = objDs.getColumn(0, "c_staffid");
				this.ds_data.setColumn(0,"욕창담당", name);
				this.ds_data.setColumn(0,"담당코드", staffid);
			}
			else
			{
				this.ds_data.setColumn(0,"욕창담당", "");
				this.ds_data.setColumn(0,"담당코드", "");
			}
			break;
		case "month_search" :
			this.fn_search_mon(objDs);
			break;
	}
}


// 간호시간 입력란 클릭 이벤트
this.mae_time_oneditclick = function(obj:nexacro.MaskEdit,e:nexacro.EditClickEventInfo)
{
  	this.div_time.set_visible(true);
	this.div_time.move(675,450);	
};


// 간호시간 입력란에서 포커스가 나가면 시간입력 팝업 죽임
this.mae_time_onkillfocus = function(obj:nexacro.MaskEdit,e:nexacro.KillFocusEventInfo)
{
	// 숫자입력 팝업창의 버튼을 클릭하면서 포커싱이 나가는 경우에도 팝업창이 보이지 않게 되는 현상 방지
	if ( !(e.newcomponent instanceof Button) ) {
		
		this.div_time.set_visible(false);
	}
};

// popupdiv 분 클릭
this.div_time_btn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	/*
	 1. 4자리 선택 또는 확인 시 div 닫기
	 2. 취소 선택 시 00으로 변경
	 3. 60이상 선택시 00으로 변경
	*/	
	// 선택한 버튼 값
	var objid = obj.id.substr(4);
	var eventid = fv_mineventid.substr(7); // edt_min08 -> 08
	
	fv_minclick = fv_minclick + 1; // 버튼 선택 횟수
	
	if(eventid < 10){
		eventid = eventid.substr(1);
	}
	//var eventid2 = nexacro.toNumber(eventid) + 1; // 끝시간 id
	
	// 4자리 선택 또는 확인 선택 시 div 닫기
	if(obj.id == "btn_select"){
		this.div_time.set_visible(false);
		var str = this.ds_data.getColumn(0,"욕창시간");
		if(str < '0600' || str > '2300' || str.substring(2) > '59') {
			this.ds_data.setColumn(0,"욕창시간", "0000");
		}else{
			//this.ds_data.setColumn(0,"욕창시간", this.ds_data.getColumn(0,"smin"+eventid));	
		}
		fv_minclick = 0;
	}else if(obj.id == "btn_cancel"){ // 취소시 0000으로 값 변경
		this.div_time.set_visible(false);
		this.ds_data.setColumn(0,"욕창시간", "0000");
		fv_minclick = 0;
	}else{
		var setStr = "";
		if(fv_minclick < 4){ // 1,2,3번째 자리 변경
			setStr = this.ds_data.getColumn(0,"욕창시간").replaceAt(fv_minclick-1,objid);
			this.ds_data.setColumn(0,"욕창시간", setStr);
		}else { // 4번째 자리 변경 & 닫고 선택횟수 0으로 변경
			setStr = this.ds_data.getColumn(0,"욕창시간").replaceAt(fv_minclick-1,objid);
			this.ds_data.setColumn(0,"욕창시간", setStr);
			
			if(setStr < '0600' || setStr > '2300' || setStr.substring(2) > '59') {
				this.ds_data.setColumn(0,"욕창시간", "0000");
			}
			
			this.div_time.set_visible(false);
			fv_minclick = 0;
		}
	}
};


// 우상단 그림 체크박스 선택 변경 -> ds_sore_part 값 변경 이벤트
// 선택항목 명칭을 욕창부위에 표시
this.ds_sore_part_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	let str = "";

	for (var i=0; i<16; i++) {
		if (i==e.col) {
			if (e.newvalue == '1') {
				str += this.sore_part_arr[i] + ',';
			}
		} else {
			if (this.ds_sore_part.getColumn(0, i) == '1') {
				str += this.sore_part_arr[i] + ',';
			}
		}
	}

	str = str.substring(0, str.length-1);	// 마지막 쉼표 제거
	// 욕창부위 란에 표시
	this.div_cont.form.txt_sore_part.set_value(str);
};

