// 리턴값 세팅
// 리턴값에 특별한 의미는 없고
// 부모창에 변경(save, delete)이 있음을 전달하는 기능
this.fn_setReturnData = function(type) 
{
	this.ds_return.clearData();
	
	// 저장, 삭제	
	var aRow = this.ds_return.addRow();
	if(type == "save"){	
		this.ds_return.setColumn(aRow, "type", "save");
	}else{		
		this.ds_return.setColumn(aRow, "type", "delete");
	}
	
	this.close(this.ds_return.saveXML());
};
