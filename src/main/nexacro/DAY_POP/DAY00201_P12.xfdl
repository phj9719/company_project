// 저장 전
this.fn_save_pre = function()
{	
	if(!Sct.Grid.hasInsertedRow(this.ds_data) && !Sct.Grid.hasUpdatedRow(this.ds_data)) {
		this.alert("저장할 항목이 없습니다.");
		return false;
	}

	// 필수항목 입력여부 체크. (욕창간호일,진행과정)
	if(!this.gfn_findSnull(this.div_cont)) return false;
	
	if(this.ds_data.getColumn(0,"욕창시점")=='0') {	// DB에 기본값 0 입력되어 있는 경우에 다른 값 선택해야 저장 가능
		this.alert("진행과정은 필수항목입니다");
		return false;
	}
	
	// 방지도구 필수입력 체크
	if (this.ds_data.getColumn(0,"방지도구1") +
		this.ds_data.getColumn(0,"방지도구2") +
		this.ds_data.getColumn(0,"방지도구3") +
		this.ds_data.getColumn(0,"방지도구4") < 1) {
		
		this.alert("방지도구를 표시하세요");
		return false;
	}

	// 욕창위치 값 세팅
	let loc = "";	// 욕창위치
	for (var i=0; i<this.ds_sore_part.colcount; i++) {
		loc += this.ds_sore_part.getColumn(0, "part"+(i+1));
	}
	this.ds_data.setColumn(0, "욕창위치", loc);

	if(Sct.Util.isEmpty(this.ds_data.getColumn(0,"so_number"))){ // 기록번호 없을시 rowtype 2(추가행)
		this.ds_data.set_updatecontrol(false);
		this.ds_data.setRowType(0,2);
		this.ds_data.set_updatecontrol(true);
	}

	if (!this.confirm("저장 하시겠습니까?"))
		return false;

	return true;
}

// 삭제 전 validation
this.fn_delete_pre = function()
{
	// 기록번호 없을 시
	if (Sct.Util.isEmpty(this.ds_data.getColumn(0,"so_number")))
	{
		this.alert("삭제할 항목이 없습니다.");
		return false;
	}

	if (!this.confirm("욕창간호기록을 삭제 하시겠습니까?"))
		return false;
		
	if (!this.confirm("욕창간호기록을 삭제하면 다시 작성해야 합니다. 그래도 삭제하시겠습니까?"))
		return false;

	this.ds_data.set_updatecontrol(false);
	this.ds_data.setRowType(0, Dataset.ROWTYPE_DELETE);
	this.ds_data.set_updatecontrol(true);
		
	return true;
};
