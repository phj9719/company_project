// 입력, 수정용 데이터셋에 데이터 초기화
this.fn_dataCopy = function()
{	
	// 기록번호
	var sonum = this.ds_data.getColumn(0,"so_number");
	
	//변경자 설정 로그인한 사람
	var name = nexacro.trim(nexacro.getApplication().gds_userInfo.getColumn(0, "c_name"));
	var id = nexacro.getApplication().gds_userInfo.getColumn(0, "c_staffid");

	// 기록번호 존재하지 않는 경우 OR 기록 존재하나 담당코드와 욕창담당 컬럼이 빈 문자열인 경우
	if(Sct.Util.isEmpty(sonum) || this.ds_data.getColumn(0,"담당코드")=="") {
		this.ds_data.setColumn(0,"담당코드",id);
		this.ds_data.setColumn(0,"욕창담당",name);
	}

	// 기록번호 존재하는 경우
	if(!Sct.Util.isEmpty(sonum)){
		//컬럼추가
		this.ds_data.addColumn("c_name","STRING",256);
		this.ds_data.addColumn("c_staffid","STRING",256);
		this.ds_data.setColumn(0,"c_name", name);
		this.ds_data.setColumn(0,"c_staffid", id);
		
		// 체위시분, 체위끝분이 빈문자열일 때 (입력에러? 이런 데이터가 실제 존재함)
		// '00'으로 세팅
		if (this.ds_data.getColumn(0, "체위시분") == "") {
			for(var i=0; i<11; i++){
				this.ds_data.setColumn(0,"smin"+(i+8),'00');		
			}
		}
		if (this.ds_data.getColumn(0, "체위끝분") == "") {
			for(var i=0; i<11; i++){
				this.ds_data.setColumn(0,"emin"+(i+9),'00');		
			}
		}		
		// 방지도구가 빈문자열일 때(입력에러? 이런 데이터가 실제 존재함)
		if (this.ds_data.getColumn(0,"방지도구") == "") {
			this.ds_data.setColumn(0,"방지도구1",0);
			this.ds_data.setColumn(0,"방지도구2",0);
			this.ds_data.setColumn(0,"방지도구3",0);
			this.ds_data.setColumn(0,"방지도구4",0);	
		}
	
 	}else{ // 기록번호 존재하지 않는 경우
