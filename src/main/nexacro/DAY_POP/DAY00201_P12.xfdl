{
	let str = "";
	for (var i=0; i<16; i++) {
		if (i==e.col) {
			if (e.newvalue == '1') {
				str += this.sore_part_arr[i] + ',';
			}
		} else {
			if (this.ds_sore_part.getColumn(0, i) == '1') {
				str += this.sore_part_arr[i] + ',';
			}
		}
	}

	if (str.length>80) {
		this.alert("욕창부위를 더 이상 선택할 수 없습니다. 다른 부위를 선택해제한 다음 시도해 주세요");
		return false;
	} 
	
	return true;
};

