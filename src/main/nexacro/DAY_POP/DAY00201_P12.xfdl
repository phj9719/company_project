	if (!this.fn_delete_pre()) return;
	
	const svcId = "delete";
	const params = "/commonController.do";
	const inDatasets = "input1=ds_data:U";
	const outDatasets = "";
	const argument = "method=delete "
				   + "sqlId=DAY00201_P12.DAY00201_P12_DS_DATA_D ";
	
	const callbackFunc = "fn_callback";
	Sct.transaction(this, svcId, params, inDatasets, outDatasets, argument, callbackFunc);
};

//콜백
this.fn_callback = function(strSvcID, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0) 
	{
		if(strSvcID == "search")
		{
			// 데이터 세팅
			this.fn_dataCopy();
			if (Sct.Util.isEmpty(this.ds_data.getColumn(0,"so_number"))
					|| this.ds_data.getColumn(0, "욕창위치").trim().length < 16) {
				this.fn_sore_part_init();
			}
		} else if(strSvcID == "save")
		{
			this.alert("저장되었습니다.");
 			this.fn_setReturnData("save");
			
		} else if(strSvcID == "delete")
		{
			this.alert("삭제되었습니다.");
 			this.fn_setReturnData("delete");
			
		} else if(strSvcID == "prev_record") // 전날 요양일지 조회
		{
			if (this.ds_data_prev_day.rowcount<1) {
				this.alert("전날 욕창간호기록이 없습니다.");
				return;
			} else {
				this.ds_data.copyRow(0, this.ds_data_prev_day, 0);
				this.ds_sore_part.copyRow(0, this.ds_sore_part_prev_day, 0);
			}
		} else if (strSvcID == "search_mon") // 욕창간호기록조회(1개월)
		{
			if (this.ds_data_mon.rowcount>0) {
				this.ds_data.copyRow(0, this.ds_data_mon, 0);
				this.ds_sore_part.copyRow(0, this.ds_sore_part_mon, 0);	
			}
		}
	}
	else
	{
		this.alert(strErrorMsg);
	}
};

