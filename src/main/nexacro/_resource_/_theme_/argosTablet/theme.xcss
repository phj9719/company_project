/* Edit/MaskEdit */
.Edit
{
	background : #ffffff;
	-nexa-border : 1px solid #bbbbbb;
	-nexa-padding : 0px 3px 3px 5px;
	-nexa-text-align : left;
	font : normal 500 16pt/normal "Noto Sans KR";
	color : #555555;
}

.MaskEdit
{
	background : #ffffff;
	-nexa-padding : 0px 3px 3px 5px;
	-nexa-border :1px solid #bbbbbb;
	-nexa-text-align : left;
	font : normal 700 16pt/normal "Noto Sans KR";
	color : #555555;
	
}

.Edit[status=focused],.Edit[status=mouseover],.MaskEdit[status=focused],.MaskEdit[status=mouseover]
{
	-nexa-border : 1px solid #6abce4;
}

.Edit[status=readonly]
{
	color : #cecece;
	-nexa-border :1px solid #bbbbbb;
}

.Edit[status=disabled],.MaskEdit[status=disabled]
{
	color : #cecece;
}
/* TextArea */
.TextArea,.TextAreaControl
{
	background : #ffffff;
	-nexa-border : 1px solid #bbbbbb;
	-nexa-padding : 5px 3px 3px 3px;
	-nexa-text-align : left;
	font : normal 500 15pt/normal "Noto Sans KR";
	color : #555555;
}

.TextArea[status=mouseover],.TextAreaControl[status=mouseover]
{
	-nexa-border : 1px solid #6abce4;
}

.TextArea[status=disabled],.TextAreaControl[status=disabled]
{
	background : #eeeeee;
	-nexa-border : 1px solid #dbdbdb;
	color : #cecece;
}

.TextArea[status=readonly],.TextAreaControl[status=readonly]
{
	background : #eeeeee;
	-nexa-border :  1px solid #bbbbbb;
	color : #cecece;
}

/* nulltext, invalidtext */
.Edit[userstatus=nulltext],
.EditControl[userstatus=nulltext],
.MaskEdit[userstatus=nulltext],
.MaskEditControl[userstatus=nulltext],
.TextArea[userstatus=nulltext],
.TextAreaControl[userstatus=nulltext]
{
