	-nexa-border : 3px solid #47afe1;
	color : #47afe1;
	font : normal 700 20pt/normal "Noto Sans KR";
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}
.Button.btn_tSelectNumber2
{
	background : #f1f1f1;
	border-radius : 32px;
	color : #555555;
	font : normal 700 20pt/normal "Noto Sans KR";
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}
.Button.btn_tDefaultSkyblue3
{
	border-radius : 25px;
	-nexa-text-align : center;
