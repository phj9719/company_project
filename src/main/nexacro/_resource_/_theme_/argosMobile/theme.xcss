}

.TitleBarControl .normalbutton
{
	-nexa-icon : URL("theme://images/btn_normal_N.png");
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.TitleBarControl .normalbutton[userstatus=pushed]
{
	background : #2196f3;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_P.png");
}

.TitleBarControl .normalbutton[status=mouseover]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_O.png");
}

.TitleBarControl .normalbutton[status=disabled]
{
	background : #64b5f6;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_normal_D.png");
}

.TitleBarControl .closebutton
{
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
	-nexa-icon : URL("theme://images/btn_close_N.png");
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.TitleBarControl .closebutton[userstatus=pushed]
{
	background : #b13138;
	-nexa-border : 1px solid transparent;
	border-radius : 50px;
