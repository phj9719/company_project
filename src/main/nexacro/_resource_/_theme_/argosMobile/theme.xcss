}


/* StatusBar */
.StatusBarControl
{
	background : #f6f6f6;
	-nexa-border : 1px solid #e0e0e0;
	border-radius : 0px 0px 3px 3px / 0px 0px 3px 3px;
	-nexa-padding : 0px 0px 0px 10px;
}

.StatusBarControl .statustext
{
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
}

.StatusBarControl .resizegrip
{
	-nexa-icon : url("theme://images/img_grip.png");
	-nexa-vertical-align : bottom;
	-nexa-text-align : center;
}

.StatusBarControl .progressbar
{
	background : #90caf9;
	-nexa-border : 5px solid transparent;
	color : #000000;
}

.StatusBarControl .progressbar .progressbaritem
{
	background : #1565c0;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.StatusBarControl .progressbar .progressbartext
{
	font : normal 700 8pt/normal "Noto Sans KR";
	color : #ffffff;
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
}


/* Static */
.Static,.StaticControl
{
	color : #333333;
	-nexa-text-align : left;
	-nexa-vertical-align : middle;
	font : normal 500 15pt/normal "Noto Sans KR";
}

.Static[status=disabled],.StaticControl[status=disabled]
{
	color : #9a9a9a;
}


/* Button */
.Button
{
	background :#ffffff;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.Button[status=mouseover]
{	
}

.Button[userstatus=pushed]
{
}

.Button[status=disabled]
{
}


/* Edit/MaskEdit */
.Edit
{
	background : transparent;
	-nexa-border : 0px none, 0px none, 1px solid #cccccc, 0px none;
	-nexa-padding : 0px 3px ;
	-nexa-text-align : left;
	font : normal 500 16pt/normal "Noto Sans KR"; 
	color : #7d7f97;
}

.MaskEdit
{
	background : #ffffff;
	-nexa-border : 0px none, 0px none, 1px solid #c0c0c0, 0px none;
	-nexa-padding : 0px 3px 0px 3px;
	color : #7d7f97;
	font : normal 500 16pt/normal "Noto Sans KR";
}

.Edit[status=focused],.Edit[status=mouseover],.MaskEdit[status=focused],.MaskEdit[status=mouseover]
{
	-nexa-border :  0px none, 0px none, 1px solid #cccccc, 0px none;
	color : #7d7f97;
}

.Edit[status=readonly]
{
	-nexa-border : 0px none, 0px none, 1px dashed #cccccc, 0px none;
}

.Edit[status=disabled],.MaskEdit[status=disabled]
{
	color : #9a9a9a;
	-nexa-border : 0px none, 0px none, 1px dashed #cccccc, 0px none;
}
/* TextArea */
.TextArea,.TextAreaControl
{
	background : #ffffff;
	-nexa-border : 1px solid #bdbdbd;
	border-radius : 2px;
	-nexa-padding : 3px 3px 3px 3px;
	-nexa-text-align : left;
}

.TextArea[status=mouseover],.TextAreaControl[status=mouseover]
{
	-nexa-border : 1px solid #1565c0;
}

.TextArea[status=disabled],.TextAreaControl[status=disabled]
{
	background : #ebebeb;
	-nexa-border : 1px solid #cccccc;
	color : #999999;
}

.TextArea[status=readonly],.TextAreaControl[status=readonly]
{
	background : #fafafa;
	-nexa-border : 1px solid #c0c0c0;
	color : #333333;
}

/* nulltext, invalidtext */
.Edit[userstatus=nulltext],.EditControl[userstatus=nulltext],.MaskEdit[userstatus=nulltext],.MaskEditControl[userstatus=nulltext],.TextArea[userstatus=nulltext],.TextAreaControl[userstatus=nulltext]
{
	color : #a9a9a9;
}
.Calendar[userstatus=invalidtext],.CalendarControl[userstatus=invalidtext],.MaskEdit[userstatus=invalidtext],.MaskEditControl[userstatus=invalidtext],.Spin[userstatus=invalidtext],.SpinControl[userstatus=invalidtext]
{
}


/* Spin */
.Spin
{
	background : #ffffff;
	-nexa-border : 1px solid #c0c0c0;
