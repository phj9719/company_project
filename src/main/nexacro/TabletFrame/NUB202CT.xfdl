 ***********************************************************************/
//초기화
this.fn_reset = function() {
	this.ds_search.setColumn(0, "DATE", Sct.Util.getToday()); // 오늘
	this.fn_search(this.fv_currentDS, this.fv_currentDS_i);
	//this.fn_search();
}

