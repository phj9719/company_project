	if(this.ds_connlist.rowcount < 1){
		//this.alert("연결된 기기가 없습니다. 기기 연결 후 측정하시기 바랍니다.");
		Sct.alert(this, "000583"); // 연결된 기기가 없습니다. 건강측정 기기연결 화면 이동 후 기기 연결하시기 바랍니다. 
