{	
	this.fn_search(this.fv_currentDS);
	//this.fn_search(this.fv_currentDS);

};

// 저장 버튼
this.fn_saveBtn = function ()
{	
	if(!this.fn_save_pre()) return;
	this.fn_save();
};

//삭제 버튼
this.fn_deleteBtn = function ()
