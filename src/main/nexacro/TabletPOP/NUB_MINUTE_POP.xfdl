<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="NUB_MINUTE_POP" width="700" height="650" titletext="분 입력 팝업" onclick="formClick" onkeyup="fn_openDebugPopup" ondevicebuttonup="NUB_MINUTE_POP_ondevicebuttonup" onload="NUB_MINUTE_POP_onload">
