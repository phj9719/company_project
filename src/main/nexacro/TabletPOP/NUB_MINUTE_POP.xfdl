                    <Static id="sta_min_select" taborder="0" text="분 선택" left="18" top="-10" width="200" height="60" textAlign="center" visible="true" onclick="formClick" cssclass="sta_tPopupTitle" background=""/>
                    <Button id="btn_00" taborder="1" text="00" left="7" top="70" width="100" height="60" cssclass="btn_tSelectNumber" onclick="btn_select_onclick"/>
