{
	this.fn_search();
};

this.div_search_cal_sdate_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
	if(!this.fn_list_search_pre()) {
		this.ds_search.setColumn(0,"DT",e.prevalue);
		return;
   } else {
		this.ds_search.setColumn(0,"DT",e.postvalue);
		this.fn_list_search();
   }
};

