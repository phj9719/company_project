			//저장후 조회후 변경된 row로 rowposition 설정
			//this.gfn_saveAfterFindRow(this.ds_list);
			//this.fn_findMstRow(this.fv_saveRefNo);
			
		}else if(strSvcID == "searchDetail"){
			this.que_set_div();
		}
	}
	else
	{
		Sct.alert(this, "", strErrorMsg);
	}
};

