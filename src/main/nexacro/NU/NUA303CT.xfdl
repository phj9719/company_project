   if(this.fn_detail_search_pre()) {
      var searchDt = this.ds_search.getColumn(0, "DT");
      var nextDt = Sct.Util.addDate(searchDt, 1);
      this.ds_search.setColumn(0, "DT", nextDt);
      this.fn_detail_search(this.ds_resident.rowposition);
