				cssStr = "expr:dataset.getColumn(currow, '" + dayArr[i] + "color') == 1 ? 'CellBf7fcff' : ''";
				break;
			default:
				cssStr = "expr:dataset.getColumn(currow, '" + dayArr[i] + "color') == 1 ? 'CellBfbfeff' : ''";
		}
		grid.setCellProperty("body", i, "cssclass", cssStr);
	}
