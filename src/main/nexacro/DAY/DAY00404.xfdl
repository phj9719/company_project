// 리포트 출력
this.fn_report = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_data.rowcount == 0)
	{
		this.alert("수급자를 선택해주세요.");
		return false;
	}
	
	/*
	 * ubireport 용 변수 
	 * ubiPreview 함수에 파라미터로 사용됨 필수 이외의 파라미터는 생략가능 
	 * 파라미터 순서는 아래 순서를 따라감
	 * jrfFile : 리포트 파일명 (필수)
	 * arg : 리포트 아규먼트 ( 세팅 예 : COMPANY#유비디시전#HOMEPAGE#http://www.ubireport.com# )
	 * mul : 멀티리포트 여부 ( 멀티리포트 사용시 true로 세팅 )
	 * mulCount : 멀티리포트 사용시 리포트 반복횟수 설정 숫자형태의 문자열
	 * pdf : pdf 프린트 바로 열기 여부 모달로 띄울시 false
	 * type : 리포트 데이타 유형 ( 0:SQL, 1:Nexacro 데이터셋, 2:화면 내 Grid(자동리포트) )
	 *		  정의하지 않을경우 0으로 자동 세팅
	 * ds : Nexacro 데이터셋 정보 ( 배열 : [["SQL01", this.ds_list], ["SQL02", this.ds_data]] 
	 *								데이터셋을 사용한 리포트 호출시 필요 )
	 * grid : 자동 리포트에서 사용하는 그리드 객체 ( 데이타 유형이 2일 경우 필요 )
	 */
	let jrfFile = "";
	switch(obj.id) 
	{
		case "btn_report1" :
			jrfFile = "DAY00404_R01.jrf";
			break;
		case "btn_report2" :
			jrfFile = "DAY00404_R02.jrf";
			break;
		case "btn_report3" :
			jrfFile = "DAY00404_R03.jrf";
			break;
		default:
	}
	
	// 미이용일 배열로 만들기
	notArr.splice(0, notArr.length);
	for (let i = 1; i <= 5; i++)
	{
		const notIn = this.ds_notIn.getColumn(0, "not" + i);
		if (notIn != undefined && notIn != null && notIn.trim() != "")
			notArr.push(this.ds_notIn.getColumn(0, "not" + i));
	}
	
	let report2Arr = this.fn_defferenceArray(timeArr, dbclArr);
	report2Arr = this.fn_defferenceArray(report2Arr, notArr);
	//trace("dbclArr : " + dbclArr);
	//trace("notArr : " + notArr);
	//trace("report2Arr : " + report2Arr);
	
	// 아규먼트 세팅 (공통)
	let arg = "P_SICODE#" + fv_sicode + "#"
			+ "P_PDATE#" + this.ds_report.getColumn(0, "print") + "#"
			+ "P_CODE#" + this.ds_data.getColumn(0, "m_code") + "#";
	
	// 급여제공 계획서에서 날짜 표시를 위한 아규먼트 세팅
	for (let i = 1; i <= this.daycount; i++)
	{
		const count = i + "";
		let val = 0;
		for (let j = 0; j < report2Arr.length; j++)
		{
			if (i == report2Arr[j])
			{
				val = 1;
				break;
			}
		}
		
		if (val == 1)
			arg += "P_" + count.padStart(2, '0') + "#O#";
		else
			arg += "P_" + count.padStart(2, '0') + "#X#";
	}
	
	// 송영일정표에서 값을 표시하기 위한 아규먼트 세팅
	let notStr = "";
	let notStr2 = "";
	for (let i = 0; i < notArr.length; i++)
	{
		if (i == 0)
			notStr += notArr[i];
		else
			notStr += "," + notArr[i];
	}
	arg += "P_NOTARR#" + notStr + "#";
	
	for (let i = 0; i < dbclArr.length; i++)
	{
		if (i == 0)
			notStr2 += dbclArr[i];
		else
			notStr2 += "," + dbclArr[i];
	}
	arg += "P_NOTARR2#" + notStr2 + "#";
	arg += "P_COUNT#" + this.ds_report.getColumn(0, "totday") + "#";
	arg += "P_DATE_FROM#" + this.ds_search.getColumn(0, "DATE_FROM") + "#";
	
	// frameTop에 있는 리포트 div 내용 갱신
	nexacro.getApplication().av_topFrame.form.components["UbiDiv"].form.ubiPreview(jrfFile, arg);
};

// 팝업 콜백
this.fn_popCallback = function (popid, strDs) {
	//trace("popid : " + popid);
	//trace("strDs : " + strDs);
	if(Sct.Util.isEmpty(strDs)) return;
	const objDs = this.gfn_createReturnDs(this, "ds_Return", strDs);
	const code1 = objDs.getColumn(0, "gubun1");
	const code2 = objDs.getColumn(0, "gubun2");
	let val1 = "";
	let val2 = "";
	if (Sct.Util.isEmpty(code1))
		val1 = "  ";
	else
		val1 = code1;
	
	if (Sct.Util.isEmpty(code2))
		val2 = "  ";
	else
		val2 = code2;
	switch(popid)
	{
		case "car" :
			if (objDs.getColumn(0, "DEL") != "1")
			{		
				this.ds_data.setColumn(0, "이용차량", val1 + val2);
			}
			else
			{
				this.ds_data.setColumn(0, "이용차량", "");
			}
