                          <Static id="sta_body3" taborder="20" text="   ▣  월 1회 이상 천장, 벽면, 창틀 및 방충망 등에 쌓인 먼지를 청소하고 있는지 ?" left="20" top="210" width="647" height="35" cssclass="sta_WF_DetailLabel02"/>
                          <Static id="sta_o2" taborder="21" left="sta_body3:-8" top="210" width="70" height="35" cssclass="sta_WF_DetailLabel02" textAlign="center"/>
                          <Static id="sta_x2" taborder="22" left="sta_o2:-1" top="210" height="35" cssclass="sta_WF_DetailLabel02" textAlign="center" width="70"/>
                          <CheckBox id="chk_o2" taborder="23" text="   O" left="sta_o2:-58" top="216" width="45" height="22" cssclass="chk_o" truevalue="1" falsevalue="0" onchanged="mainChk_onchanged"/>
