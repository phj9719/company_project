{
	const num = obj.id.substr(5);

	const cRow = parseInt(num) - 1;
	const offset = 0;
	const value = e.postvalue;
	const str1 = this.ds_data.getColumn(0, "점검항목");
	
	switch (obj.id) 
	{
		// 양호
		case "chk_o" + num:
			if (value == 1)
