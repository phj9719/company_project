this.form_onload = function (obj:Form, e:LoadEventInfo)
{
	// 폼로드시 공통함수
	Sct.onLoad(obj, e);
};

// 사용자 정의 폼 로드 함수(form_init 고정)
this.form_init = function (obj)
{
	this.fn_search();
};

