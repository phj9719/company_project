};





this.btn_onOff_onclick = function (obj:Button, e:nexacro.ClickEventInfo)
{
	if (this.st_msg.visible) 
	{
		this.st_msg.set_visible(false);
	}
	else 
	{
		this.st_msg.set_visible(true);
	}
}
;
